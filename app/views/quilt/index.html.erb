

<%= stylesheet_link_tag 'quilt/grid.css', media: 'all' %>
<%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/jquery.imagesloaded/3.2.0/imagesloaded.pkgd.min.js' %>
<%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/masonry/3.3.2/masonry.pkgd.min.js' %>
<%= javascript_include_tag 'quilt/main.js' %>

<div class='container-fluid'>
  <div class='grid'>

    <div class='grid-sizer'></div>

    <% @squares.each do |square| %>

    <div class='grid-item'>

      <img src="<%= square.photo_thumb_url %>" alt='thumb' />

    </div>

    <% end %>

  </div>
</div>

